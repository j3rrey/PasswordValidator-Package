{
    "name": "j3rrey/laravel-password-validation",
    "description": "Simple extendable password validation rules for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeremy Bruns",
            "email": "bruns.jeremy@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "J3rrey\\PasswordValidation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "j3rrey\\PasswordValidation\\PasswordValidationServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan asset:publish J3rrey\\PasswordValidation\\PasswordValidationServiceProvider"
        ]
    }
}
